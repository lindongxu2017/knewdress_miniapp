/* pages/mergeResult/mergeResult.wxss */
.result-img {
  position: absolute;
  width: 100%;
  background-color: #f5f5f5;
}
.operation-wrapper {
  position: absolute;
  width: 100%;
  height: 70px;
  left: 0;
  bottom: 0;
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  -webkit-justify-content: space-around;
  align-items: center;
  -webkit-align-items: center;
}
.operation-wrapper > view {
  text-align: center;
}
.operation-wrapper > view > view {
  font-size: 28rpx;
  color: #767676;
}
.operation-wrapper image {
  width: 50rpx;
  height: 50rpx;
}