/* pages/wardrobeSimilar/wardrobeSimilar.wxss */
.img {
    width: 100%;
    height: auto;
    display: block;
    margin-bottom: 45rpx;
}

.lable-list {
    padding-left: 30rpx;
    padding-right: 10rpx;
    font-size: 0;
}

.lable-list .edit-icon {
    width:68.75rpx;
    height:55rpx;
    display: inline-block;
    vertical-align: middle;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
.lable-list .lable {
    padding:0 30rpx;
    border-radius:88rpx;
    border:1px solid #F0F3F7;
    display:inline-block;
    color:#474747;
    margin-right:22px;
    margin-bottom: 20rpx;
    line-height:55rpx;
    background-color:#F0F3F7;
    font-size: 28rpx;
}

.cell-title {
    font-size: 32rpx;
    font-weight: bold;
    padding: 10rpx 30rpx;
    color: #474747;
}